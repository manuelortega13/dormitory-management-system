{
  "version": 2,
  "builds": [
    {
      "src": "dist/dorm/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://node-server-production-e00e.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/dorm/browser/index.html"
    }
  ]
}
